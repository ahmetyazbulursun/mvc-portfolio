@using my_website.Models.Entity
@model List<Tbl_Admin>

@{
    ViewBag.Title = "Admin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="p-5">
    <table class="table table-borderless table-responsive-sm table-responsive-md bg-light">
        <thead align="center">
            <tr>
                <th>Username</th>
                <th>Password</th>
                <th>Operations</th>
            </tr>
        </thead>
        @foreach (var x in Model)
        {
            <tbody align="center">
                <tr>
                    <td>@x.USERNAME</td>
                    <td>@x.PASSWORD</td>
                    <td><a href="/Admin/AdminUpdate/@x.ID" class="btn btn-outline-success">Update</a></td>
                </tr>
            </tbody>
        }
    </table>
</div>
<div style="height: 710px"></div>