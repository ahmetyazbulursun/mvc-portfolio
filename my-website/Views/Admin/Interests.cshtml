@using my_website.Models.Entity
@model List<Tbl_Interests>

@{
    ViewBag.Title = "Interests";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="p-5">
    <div class="float-start mb-3">
        <a href="/Admin/AddInterest" class="btn btn-info">Add Interest</a>
    </div>
    <table class="table table-borderless table-responsive-sm table-responsive-md bg-light">
        <thead align="center">
            <tr>
                <th>Interest</th>
                <th colspan="2">Operations</th>
            </tr>
        </thead>
        @foreach (var x in Model)
        {
            <tbody align="center">
                <tr>
                    <td>@x.INTEREST</td>
                    <td><a href="/Admin/InterestUpdate/@x.ID" class="btn btn-outline-success">Update</a></td>
                    <td><a href="/Admin/InterestDelete/@x.ID" class="btn btn-outline-danger">Delete</a></td>
                </tr>
            </tbody>
        }
    </table>
</div>
<div style="height: 520px"></div>