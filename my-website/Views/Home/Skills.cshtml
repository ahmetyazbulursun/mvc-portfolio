@using my_website.Models.Entity
@model List<Tbl_Skills>

<section id="skills">
    <div class="container">
        <div class="row mb-5">
            <div class="col-lg-10 col-xl-8 mx-lg-auto text-center">
                <div class="section-title h2 mb-3">
                    <h2 class="mb-0">Skills</h2>
                    <span class="title-letter">S</span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12 mb-4 mb-md-0">
                @foreach (var x in Model)
                {
                    <p class="h6 mb-1">@x.SKILL</p>
                    <div class="progress mb-4 rounded-0" style="height:5px">
                        <div class="progress-bar" role="progressbar" style="width: @x.VALUE" aria-valuenow="@x.VALUE" aria-valuemin="0" aria-valuemax="100"></div>
                    </div>
                }
            </div>
        </div>
    </div>
</section>
