@using my_website.Models.Entity
@model List<Tbl_Certificate>

<section id="certificates" class="">
    <div class="container">
        <div class="row mb-5">
            <div class="col-lg-10 col-xl-8 mx-lg-auto text-center">
                <div class="section-title h2 mb-3">
                    <h2 class="mb-0">Certificates</h2>
                    <span class="title-letter">C</span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12 mb-4 mb-md-0">
                @foreach (var x in Model)
                {
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item bg-light"><iconify-icon icon="simple-icons:udemy"></iconify-icon><a href="@x.LINK" target="_blank" rel="nofollow" class="pl-2 text-dark">@x.NAME</a></li>
                    </ul>
                }
            </div>
        </div>
    </div>
</section>

